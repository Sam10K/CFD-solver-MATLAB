% Boundary condition file %
% Currently supports wall value and zero gradient boundary conditions%
% Requires type and value fields %

internal field
{
 list_type uniform;
 list_values 0;
}

ZONE-LEFT
{
  type wall;
  value 0;
}

ZONE-RIGHT
{
  type wall;
  value	0;
}

ZONE-TOP
{
  type wall;
  value	0;
}

ZONE-BOTTOM
{
  type wall;
  value	0;
}
